# --- Keycloak (authentification SSO) ---
KEYCLOAK_URL=https://sso.wikiplatform.app
KEYCLOAK_REALM=saas
KEYCLOAK_CLIENT_ID=portal
KEYCLOAK_CLIENT_SECRET={{ keycloak_portal_secret }}

